{{>header}}
{{>navbar}}
<section class="container my-3">
    <h2>Add New User (POST)</h2>
    <form method="post">
        <div class="mb-3">
            <input class="form-control" name="name" placeholder="Enter your name">
        </div>
        <div class="mb-3">
            <input class="form-control" name="age" placeholder="Enter your age">
        </div>
        <div class="mb-3">
            <input class="form-control" name="email" placeholder="Enter your email">
        </div>
        <div class="mb-3">
            <input class="form-control" name="address" placeholder="Enter your address">
        </div>
        <div class="mb-3">
            <button class="btn btn-primary">Add Data</button>
        </div>
    </form>
</section>
{{>footer}}
